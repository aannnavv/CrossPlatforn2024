class MyClass {
    private int number;
    private String name;

    // Конструктори
    public MyClass() {
        this.number = 0;
        this.name = "";
    }

    public MyClass(int number, String name) {
        this.number = number;
        this.name = name;
    }

    // Методи
    public void display() {
        System.out.println("Number: " + number);
        System.out.println("Name: " + name);
    }
}
